  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.16",
    "commander": "^11.1.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "postcss": "^8.4.31",
    "puppeteer": "^24.16.0",
