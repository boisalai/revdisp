{
  "name": "revdisp-calculator",
  "version": "1.0.0",
  "description": "Calculateur du revenu disponible au Qu√©bec",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build",
    "validate": "npx tsx src/lib/validation/ValidationRunner.ts",
    "validate:watch": "npx tsx --watch src/lib/validation/ValidationRunner.ts",
    "validate:ramq": "npx tsx src/lib/validation/RunRAMQValidation.ts",
    "validate:rrq": "npx tsx src/lib/validation/RunRRQValidation.ts",
    "validate:ei": "npx tsx src/lib/validation/RunEIValidation.ts",
    "validate:rqap": "npx tsx src/lib/validation/RunRQAPValidation.ts",
    "validate:fss": "npx tsx src/lib/validation/RunFSSValidation.ts",
    "check": "./scripts/check-before-deploy.sh",
    "test-prod": "npm run build && cd out && python -m http.server 8080",
    "validate:ei:legacy": "node scripts/validate-ei.js",
    "validate:ei:automated": "node scripts/validate-ei-automated.js",
    "validate:mass": "npx tsx src/lib/validation/ValidationCLI.ts",
    "validate:generate": "npx tsx src/lib/validation/ValidationCLI.ts generate",
    "validate:high-volume": "npx tsx src/lib/validation/ValidationCLI.ts validate",
    "validate:continuous": "npx tsx src/lib/validation/ValidationCLI.ts continuous",
    "validate:status": "npx tsx src/lib/validation/ValidationCLI.ts status",
    "validate:analyze": "npx tsx src/lib/validation/ValidationCLI.ts analyze",
    "validate:quebec-tax": "npx tsx src/lib/validation/TestQuebecTaxValidation.ts",
    "test:quebec-tax": "npx tsx src/lib/validation/QuickQuebecTaxTest.ts"
  },
  "dependencies": {
    "@types/d3": "^7.4.3",
